package com.vantiq.engines
multi partition PROCEDURE EngineMonitor.writeEngineStatus()

log.debug("Writing Engine State to the database")

for (engineState in EngineMonitor_SpeedEvent_EventHandler_DetectMalfunction_UpdateStateState) {
    UPSERT com.vantiq.engines.SystemStatus(engineState.value)
}

for (engineState in EngineMonitor_TempEvent_EventHandler_DetectMalfunction_UpdateStateState) {
    UPSERT com.vantiq.engines.SystemStatus(engineState.value)
}