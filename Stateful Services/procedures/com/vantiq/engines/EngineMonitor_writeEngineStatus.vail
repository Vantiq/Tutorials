package com.vantiq.engines
//Write the engine state to the database
multi partition PROCEDURE EngineMonitor.writeEngineStatus()

log.debug("Writing DetectMalfunction_UpdateTempAndSpeedState to the database")

var state = DetectMalfunction_UpdateTempAndSpeedState
for (engineState in state) {
    UPSERT com.vantiq.engines.SystemStatus(engineState.value)
}